.homePage {
  width: 100%;
  height: 367px;
  left: 0px;
  top: 0px;
}
.homePage .status {
  width: 100%;
}
.homePage .homePage_header {
  height: 85rpx;
  width: 100%;
  line-height: 85rpx;
  padding-left: 20px;
  font-size: 16px;
}
.yellowPage {
  background: linear-gradient(180deg, #fecf3c 0%, #f7f8fa 100%);
}
.grayPage {
  background: linear-gradient(180deg, #cbd0db 0%, #f7f8fa 100%);
}
.bluePage {
  background: linear-gradient(180deg, #8fb2ff 0%, #f7f8fa 100%);
}